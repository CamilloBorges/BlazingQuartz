{
  "ConnectionStrings": {
    "BlazingQuartzDb": "DataSource=BlazingQuartzApp.db;Cache=Shared"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "BlazingQuartz": {
    "DataStoreProvider": "Sqlite",
    "AutoMigrateDb": true,
    "AllowedJobAssemblyFiles": [
      "BlazingQuartz.Jobs"
    ]
  },
  "AllowedHosts": "*"
}

